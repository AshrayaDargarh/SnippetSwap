{
  "version": 2,
  "name": "snippetswap",
  "builds": [{ "src": "build/**", "use": "@vercel/static" }],
  "routes": [
    {
      "src": "/public",
      "dest": "index.js" 
    },
    {
      "src": "/public",
      "dest": "index.js" 
    },
    {
      "src": "/",
      "dest": "/build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "build/$1"
    }
  ]
}
